#modal-dialog
  background-color rgba(0,0,0, 0)
  transition 0.3s ease-out
  padding-top 30px
  overflow scroll
  position fixed
  display block
  z-index 1000
  height 100%
  width 100%
  outline 0
  bottom 0
  right 0
  left 0
  top 0
  #modal-content
    background-color white
    border-radius 10px
    position relative
    margin 0 auto
    outline 0
    width 82%
    transform translateY(-25%)
    transition 0.3s ease-out
#hidden
  display none
.fadeqwe
  transform translate(0, 0)!important
  transition 0.3s ease-out
.fadetyu
  background-color rgba(0,0,0, 0.5)!important
  transition 0.3s ease-out
